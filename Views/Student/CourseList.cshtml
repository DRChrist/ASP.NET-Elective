@model IEnumerable<StudentApplication.Models.Course>
@{
    Viewbag.Title = "Courses";
    Layout = "_Layout";
}
    <a asp-controller="Student" asp-action="Create" class="btn btn-success">New Student</a>        
    <table class="table table-striped" "table-hover" "table-condensed">
        <tr>
        <th>@Html.DisplayNameFor(Models => Models.CourseID)</th>
        <th>@Html.DisplayNameFor(Models => Models.Title)</th>
        <th>@Html.DisplayNameFor(Models => Models.Credits)</th>
        </tr>
    @foreach(var c in Model)
        {
        <tr>
            <td> @c.Title </td>
            <td> @c.Title </td>
            <td> @c.Credits </td>
        </tr>
        } 
    </table>